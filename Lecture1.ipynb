# Application of Logical Operators with if elif else for multiple conditions
age_at_school=10
sarim_age=int(input("How Old is Your Baby :"))
if sarim_age==age_at_school:
    print("Your Baby Can Join School")
elif sarim_age>age_at_school:
    print("Your Baby is Older Enough to Join School")
else:
    years_left=age_at_school - sarim_age  
    print(f"Your Baby is Too Young for School, Wait for {years_left} years")


